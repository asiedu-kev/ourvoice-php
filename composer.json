{
    "name": "shadonet/ourvoice-php",
    "description": "Ourvoice Api REST API client for php",
    "type": "library",
    "version": "1.0.0-dev",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "Ourvoice\\": "src/"
        }
    },
    "authors": [
        {
            "name": "TIC ABC"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "php-http/mock-client": "^1.5",
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
